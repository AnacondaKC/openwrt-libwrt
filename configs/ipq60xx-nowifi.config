##################################################
### 1. 设备平台配置
##################################################
# 目标平台选择
CONFIG_TARGET_qualcommax=y
CONFIG_TARGET_qualcommax_ipq60xx=y
CONFIG_TARGET_MULTI_PROFILE=y

# 设备型号选择 (兆能M2)
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_zn_m2=y

# 禁用其他设备型号
# CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_cmiot_ax18 is not set
# CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_glinet_gl-ax1800 is not set
# CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_glinet_gl-axt1800 is not set
# CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_ax1800-pro is not set
# CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_ax6600 is not set
# CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-07 is not set
# CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_qihoo_v6 is not set
# CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_redmi_ax5 is not set
# CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_redmi_ax5-jdcloud is not set
# CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_xiaomi_ax1800 is not set


##################################################
### 2. 网络核心功能
##################################################
# === 2.1 NSS硬件加速 ===
CONFIG_NSS_DRV_PPTP_ENABLE=y
CONFIG_NSS_DRV_SHAPER_ENABLE=y
CONFIG_NSS_FIRMWARE_VERSION_12_2=y

# === 2.2 防火墙 ===
CONFIG_PACKAGE_firewall4=y
CONFIG_PACKAGE_kmod-nft-fullcone=y    # 全锥形NAT支持
CONFIG_PACKAGE_kmod-nft-tproxy=y      # 透明代理支持

# === 2.3 透明代理 ===
CONFIG_PACKAGE_kmod-nft-fib=y         # FIB查询支持
CONFIG_PACKAGE_kmod-nft-socket=y
CONFIG_PACKAGE_kmod-nft-nat=y         # NAT重定向
CONFIG_PACKAGE_kmod-nft-redir=y       # 端口重定向

# === 2.4 网络诊断工具 ===
CONFIG_PACKAGE_ip-full=y              # 完整版iproute2
CONFIG_PACKAGE_tc-tiny=y              # 流量控制工具


##################################################
### 3. 加密与压缩模块
##################################################
# --- 加密算法支持 ---
CONFIG_PACKAGE_kmod-crypto-acompress=y
CONFIG_PACKAGE_kmod-crypto-aead=y
CONFIG_PACKAGE_kmod-crypto-ccm=y
CONFIG_PACKAGE_kmod-crypto-cmac=y
CONFIG_PACKAGE_kmod-crypto-ctr=y
CONFIG_PACKAGE_kmod-crypto-gcm=y
CONFIG_PACKAGE_kmod-crypto-geniv=y
CONFIG_PACKAGE_kmod-crypto-gf128=y
CONFIG_PACKAGE_kmod-crypto-ghash=y
CONFIG_PACKAGE_kmod-crypto-hmac=y
CONFIG_PACKAGE_kmod-crypto-manager=y
CONFIG_PACKAGE_kmod-crypto-michael-mic=y
CONFIG_PACKAGE_kmod-crypto-null=y
CONFIG_PACKAGE_kmod-crypto-rng=y
CONFIG_PACKAGE_kmod-crypto-seqiv=y
CONFIG_PACKAGE_kmod-crypto-sha3=y
CONFIG_PACKAGE_kmod-crypto-sha512=y


##################################################
### 4. 系统工具与服务
##################################################
# :: 基础工具 ::
CONFIG_BUSYBOX_CUSTOM=y               # 自定义BusyBox
CONFIG_PACKAGE_curl=y                 # cURL网络工具
CONFIG_PACKAGE_resolveip=y            # DNS解析工具
CONFIG_PACKAGE_unzip=y                # ZIP解压工具

# :: 网络诊断工具 ::
CONFIG_PACKAGE_arp-scan=y             # ARP扫描工具
CONFIG_PACKAGE_ethtool=y              # 网卡诊断工具

# :: 代理工具 ::
CONFIG_PACKAGE_hysteria=m             # Hysteria协议支持

# :: 系统服务 ::
CONFIG_PACKAGE_kmod-inet-diag=y       # 网络诊断支持
CONFIG_PACKAGE_kmod-netlink-diag=y    # Netlink诊断
CONFIG_PACKAGE_kmod-tun=y             # TUN/TAP设备支持
CONFIG_PACKAGE_ucode-mod-lua=y        # Lua脚本支持
CONFIG_PACKAGE_ucode-mod-rtnl=y       # 实时网络链接
CONFIG_PACKAGE_ucode-mod-uloop=y      # 事件循环支持


##################################################
### 5. WEB界面与插件
##################################################
# ~~~ LuCI核心框架 ~~~
CONFIG_PACKAGE_luci-compat=y
CONFIG_PACKAGE_luci-lib-base=y
CONFIG_PACKAGE_luci-lib-ip=y
CONFIG_PACKAGE_luci-lib-jsonc=y
CONFIG_PACKAGE_luci-lib-nixio=y
CONFIG_PACKAGE_luci-lua-runtime=y

# ~~~ 主题与界面优化 ~~~
CONFIG_PACKAGE_luci-theme-argon=y
CONFIG_PACKAGE_luci-app-argon-config=y
CONFIG_PACKAGE_luci-i18n-argon-config-zh-cn=y

# ~~~ 功能插件 ~~~
CONFIG_PACKAGE_luci-app-cpufreq=y              # CPU频率控制
CONFIG_PACKAGE_luci-i18n-cpufreq-zh-cn=y       # CPU频率控制中文
CONFIG_PACKAGE_luci-app-homeproxy=y            # Homeproxy核心
CONFIG_PACKAGE_homeproxy=y                     # Homeproxy后端
CONFIG_PACKAGE_luci-i18n-homeproxy-zh-cn=y     # Homeproxy中文
CONFIG_PACKAGE_homeproxy_INCLUDE_Shadowsocks_Rust=y  # Shadowsocks支持
CONFIG_PACKAGE_homeproxy_INCLUDE_Xray=y        # Xray协议支持


##################################################
### 6. 库依赖
##################################################
# === 核心库 ===
CONFIG_PACKAGE_libatomic=y
CONFIG_PACKAGE_libcap=y
CONFIG_PACKAGE_libcap-ng=y
CONFIG_PACKAGE_libstdcpp=y
CONFIG_PACKAGE_libuv=y
CONFIG_PACKAGE_libwebsockets-full=y
CONFIG_PACKAGE_zlib=y

# === 网络库 ===
CONFIG_LIBCURL_COOKIES=y
CONFIG_LIBCURL_CRYPTO_AUTH=y
CONFIG_LIBCURL_FILE=y
CONFIG_LIBCURL_FTP=y
CONFIG_LIBCURL_HTTP=y
CONFIG_LIBCURL_NGHTTP2=y
CONFIG_LIBCURL_OPENSSL=y
CONFIG_LIBCURL_PROXY=y
CONFIG_LIBCURL_TFTP=y
CONFIG_LIBCURL_THREADED_RESOLVER=y
CONFIG_LIBCURL_TLS_SRP=y
CONFIG_LIBCURL_UNIX_SOCKETS=y
CONFIG_PACKAGE_libcurl=y
CONFIG_PACKAGE_libopenssl-conf=y
CONFIG_PACKAGE_libopenssl-legacy=y
CONFIG_PACKAGE_libnghttp2=y

# === 脚本引擎 ===
CONFIG_PACKAGE_liblua=y
CONFIG_PACKAGE_lua=y
CONFIG_PACKAGE_liblucihttp-lua=y
CONFIG_PACKAGE_libubus-lua=y
CONFIG_PACKAGE_libuci-lua=y

# === 其他功能库 ===
CONFIG_PACKAGE_libbpf=y
CONFIG_PACKAGE_libelf=y
CONFIG_PACKAGE_libiperf3=y
CONFIG_PACKAGE_libncurses=y
CONFIG_PACKAGE_terminfo=y


